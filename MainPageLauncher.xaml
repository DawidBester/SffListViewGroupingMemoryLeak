<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="SfListViewGroupingMemoryLeak.MainPageLauncher"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  >
    <ContentPage.Content>
        <Grid HorizontalOptions="Fill" VerticalOptions="Fill">
            <Button
                x:Name="MainPageLauncherButton"
                BackgroundColor="White"
                Clicked="MainPageLauncherButton_OnClicked"
                Text="Press to Launch MainPage"
                TextColor="Black" />
        </Grid>
    </ContentPage.Content>
</ContentPage>
